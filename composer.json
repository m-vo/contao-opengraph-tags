{
  "name": "mvo/contao-opengraph-tags",
  "type": "contao-bundle",
  "description": "Add opengraph tags to your pages in Contao Open Source CMS",
  "keywords": [
    "contao",
    "opengraph",
    "tags",
    "SEO"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Moritz Vondano",
      "homepage": "https://github.com/m-vo/contao-opengraph-tags"
    }
  ],
  "require": {
    "php": ">=7.1",
    "contao/core-bundle": "^4.4",
    "symfony/twig-bundle": "^3.4 || ^4.1 || ^5.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "Mvo\\ContaoOpenGraphTags\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Mvo\\ContaoOpenGraphTags\\ContaoManager\\Plugin"
  }
}